gcloud compute ssh --project=<project-name> --zone=<zone-name> <instance-name>


deletion:
gcloud compute instance-groups managed delete <instance-group-name> --region=<region-name> --project=PROJECT_ID
gcloud compute instance-groups managed delete <instance-group-name> --zone=<zone-name> --project=PROJECT_ID
gcloud compute instance-groups list-instances <instance-group-name> --region=<region-name> --project=PROJECT_ID
gcloud compute instances delete INSTANCE_NAME --zone=<zone-name> --project=PROJECT_ID


get terraform output:
gcloud beta resource-config bulk-export --project=PROJECT_ID --resource-format=terraform


